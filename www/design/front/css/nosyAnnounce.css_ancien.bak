@charset "utf-8";
/* CSS Document */

#middleContent {
	float:left;
	height:1%;
	/*margin-left:10px;*/
	width:650px;
}

.row {
	display:inline-block;
	width:100%;
	background:#f9fafc;
}
.read {

}

.header {
	float:left;
	padding-bottom:5px;
	width:100%;
}
.header h3 {
	background:#FFFFFF url(../images/v5/hdrBg_blue.gif) repeat-x scroll 0 0;
	font-size:15px;
	font-weight:bold;
	color:#18506c;
	height:25px;
	overflow:hidden;
	padding:4px 0 0 10px;
}
.singleColumnBig {
	color:#000000;
	float:left;
	font-size:12px;
	margin:0 10px 0 0;
	width:650px;
}

.singleColumnBig h3 {
	color:#184059;
	font-family:Georgia,Serif;
	font-size:12px;
	font-weight:normal;
	margin:5px 0;
}
.singleColumnBig h3 i{
	color:#A97E6E;
}
.singleColumnBig h3 span{
	color:#A97E6E;
}

.singleColumnBig #annonceForm {
	width:650px;
}

/*------------------------------------*/
#middleCol .box {
	background:transparent url(../images/v6/bg-box-inner.gif) repeat-y scroll 0 0;
}
.box {
	margin:0 0 10px;
}

#middleCol .title_full {
	background:transparent url(../../img/0/bg-titlebar-full.gif) no-repeat scroll 0 0;
}

#middleCol .title {
	background:transparent url(../../img/0/bg-titlebar.gif) no-repeat scroll 0 0;
	height:34px;
	padding:0 0 10px;
}

#box_cat .box_inner {
	margin:0 0 0 25px;
	width:630px;
}
.box .box_inner, .box .box_nofond {
	overflow:hidden;
}

#box_cat .col3 {
	float:left;
	margin:0 10px 0 0;
	width:197px;
}
#box_cat .cat_retract {
	background:transparent url(/include/img/0/line-double.gif) repeat-x scroll 0 bottom;
	margin:0 0 10px;
	padding:0 0 5px;
}
#box_cat .cat_box {
	background:transparent url(/include/img/0/box-categories/bg-box-cat.gif) no-repeat scroll 0 0;
	overflow:hidden;
	padding:9px 0 0;
	width:197px;
}

.annonceContent{
	padding-top:10px;
	width:640px;
	margin:0 0 0 10px;
}
#annonceForm p{
	margin:0 0 10px 0;
}
#annonceForm .annonceTitre {
	color:#184059;
	font-family:Georgia,Serif;
	font-size:105%;
	font-weight:normal;
	margin-bottom:10px;
	padding:2px 0 0;
}

.annonceCriteriaLine {
	margin-bottom:10px;
	width:100%;
}

.annonceCriteria {
	display:block;
	float:left;
	margin-right:10px;
}


.viewResult {
	background-color:#F5F3ED;
	border:1px solid #ABADB3;
	color:#184059;
	font-family:Georgia,Serif;
	font-size:12px;
	font-weight:normal;
	padding:5px 0 5px 10px;
	width:640px;
}

.viewResult p {
	/*margin-left:5px;*/
}
.viewResult p .viewTitre{
	color:#18506C;
	float:left;
	font-family:"Trebuchet MS",Arial,Sans-serif;
	font-size:16px;
	font-weight:bold;
	padding-bottom:10px;
}
.viewResult .view {
	float:left;
	padding:8px 15px 0 0;
}

/*footer pagination*/
.regText, .regTextPale, .regTextPaleBlue, .regTextBlue, .regTextBlueMast, .crumbsBlue {
	font-size:10px;
	font-weight:bold;
}
.regTextPale {
	color:#A97E6E;
	line-height:12px;
}
.commFoot {
	background:transparent url(../images/v5/comm_foot.gif) no-repeat scroll 0 0;
	border:medium none;
	border-collapse:collapse;
	height:38px;
	width:650px;
}

.commFoot3 {
	float:right;
	background-position:-300px 0;
	border:medium none;
	border-collapse:collapse;
	height:38px;
	padding:10px 0 0 0;
	width:330px;
}
.annonceFoot {
	padding:10px 0 0 10px;
	float:left;
	width:200px;
}

.commFoot .commFoot3 ul.pageNumbers {
	float:right;
	margin:-5px 0 0;
	position:relative;
}

.commFoot .static {
	float:left;
	padding:10px;
	width:300px;
}

ul.pageNumbers li.lastPage, ul.pageNumbers li.prevButton, ul.pageNumbers li.nextButton,
ul.pageNumbers li.firstButton, ul.pageNumbers li.lastButton {
	border:medium none;
}
ul.pageNumbers li {
	display:inline;
	border-right:1px solid #FFFFFF;
	float:left;
	margin:0;
	padding:0 7px;
}

a.pageFirst,
a.pageLast,
a.pageNext,
a.pagePrevious {
	display:block;
	height:21px;
	line-height:12px;
	text-indent:-9000px;
	width:22px;
	text-indent:-9000px;
}

ul.pageNumbers li.firstButton,
ul.pageNumbers li.nextButton {
	padding-right:1px;
}
ul.pageNumbers li.lastButton,
ul.pageNumbers li.prevButton {
	padding-left:1px;
}

ul.pageNumbers li.lastPage,
ul.pageNumbers li.noButton {
	padding-top:3px;
}


a.pageFirst {
	background:transparent url(../images/v5/btn_first.png) no-repeat scroll 0 0;
}	
a.pageFirst:hover {
	background:transparent url(../images/v5/btn_first.png) no-repeat scroll 0 -21px;
}
a.pageFirst:active {
	background:transparent url(../images/v5/btn_first.png) no-repeat scroll 0 -42px;
}
a.pageLast {
	background:transparent url(../images/v5/btn_last.png) no-repeat scroll 0 0;
}	
a.pageLast:hover {
	background:transparent url(../images/v5/btn_last.png) no-repeat scroll 0 -21px;
}
a.pageLast:active {
	background:transparent url(../images/v5/btn_last.png) no-repeat scroll 0 -42px;
}
a.pagePrevious {
	background:transparent url(../images/v5/btn_back.png) no-repeat scroll 0 0;
}	
a.pagePrevious:hover {
	background:transparent url(../images/v5/btn_back.png) no-repeat scroll 0 -21px;
}
a.pagePrevious:active {
	background:transparent url(../images/v5/btn_back.png) no-repeat scroll 0 -42px;
}
a.pageNext {
	background:transparent url(../images/v5/btn_next.png) no-repeat scroll 0 0;
}
a.pageNext:hover {
	background:transparent url(../images/v5/btn_next.png) no-repeat scroll 0 -21px;
}
a.pageNext:active {
	background:transparent url(../images/v5/btn_next.png) no-repeat scroll 0 -42px;
}

.img_photo .extra_etiquette {
	background:transparent url(../images/v6/bg-nb-img.gif) no-repeat scroll 0 0;
	color:#FFFFFF;
	display:block;
	font-size:83%;
	height:16px;
	left:10px;
	overflow:hidden;
	padding:0 5px;
	margin-top:-135px;
	position:absolute;
	text-align:center;
}
.result_foot {
	background:#F5F3ED none repeat scroll 0 0;
	font-size:10px;
	overflow:hidden;
	padding:0 12px 5px;
}
.regTextPaleNormal {
	color:#A97E6E;
	font-size:10px;
	font-weight:normal;
}
.borderLeftInline {
	border-left:1px solid #A97E6E;
	display:inline;
	padding-left:5px;
}
.borderNoneInline {
	display:inline;
	padding-right:5px;
	vertical-align:middle;
}
a.hotTopics {
	color:#0083B7;
	font-size:10px;
	font-weight:normal;
	text-decoration:none;
}
.result_foot ul {
	margin-top:8px;
	text-align:right;
	float:left;
}

.result_foot .float_r {
	width:45%;
}
.bt_add {
	font-size:10px;
}
.float_r {
	float:right !important;
}
